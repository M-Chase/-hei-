<!--pages/navbarData.wxml-->
<view class='nav-wrap' style='height: {{height*2 + 20}}px;'>
   
  <view class='nav-title' style='line-height: {{height*2 + 44}}px;'>{{navbarData.title}}</view>
  <view style='display: flex; justify-content: space-around;flex-direction: column;'>
     
    <view class='nav-capsule' style='height: {{height*2 + 44}}px;' wx:if='{{navbarData.showCapsule}}'>  
      <view class="{{!share && !near ? 'nav-border' : ''}}">
        <view bindtap='_navback' wx:if='{{!share}}'>
          <image src='../image/return.png' mode='aspectFill' class='back-pre'></image>
        </view>
        <view class='navbar-v-line' wx:if='{{!share && !near}}'></view>
        <view bindtap='_backhome' wx:if='{{!near}}'>
          <image src='../image/home.png' mode='aspectFill' class='back-home'></image>
        </view>
      </view>
      
    </view>

  </view>
</view>
